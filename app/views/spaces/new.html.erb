<h1>Spaces#new</h1>
<%= form_for @space do |f| %>

 <div class="newspace">
    <h1 class="new-space-title">Add Your Home</h1>

    <div class="form_field">
      <%= f.label :capacity %><br>
      <%= f.number_field :capacity %>
    </div>

    <div class="form_field">
      <%= f.label :address %><br>
      <%= f.text_field :address, placeholder: "121 Street Lane, Toronto, ON" %>
    </div>

    <div class="form_field">
      <%= f.label :garbaje_day %><br>
      <%= f.text_field :garbaje_day, placeholder: "e.x. Tuesday" %>
    </div>

    <div class="submit">
      <%= f.submit %>
    </div>
</div>

<% end %>
